        <RadioButton x:Name="protokol" Content="Protokol" HorizontalAlignment="Left" Margin="32,91,0,0" VerticalAlignment="Top" Width="210" IsChecked="True"/>
        <RadioButton x:Name="oplaty_drogowe" Content="Opłaty drogowe" HorizontalAlignment="Left" Margin="32,111,0,0" VerticalAlignment="Top" Width="190"/>
        <RadioButton x:Name="notatki" Content="Notatki" HorizontalAlignment="Left" Margin="32,131,0,0" VerticalAlignment="Top" Width="190"/>
        <Button x:Name="skanuj" Content="Skanuj" HorizontalAlignment="Left" Margin="222,91,0,0" VerticalAlignment="Top" Width="123" Click="Button_Click" Height="55"/>
        <TextBox x:Name="numer_zlecenia" HorizontalAlignment="Left" Height="23" Margin="32,47,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="313" CharacterCasing="Upper"/>
        <Label Content="Numer zlecenia (ew. numer rejestracyjny)" HorizontalAlignment="Left" Margin="32,21,0,0" VerticalAlignment="Top" Width="245"/>
    </Grid>
</Window>
